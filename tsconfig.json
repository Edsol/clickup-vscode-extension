{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["ES6", "DOM"],
    "jsx": "preserve",
    "moduleResolution": "node",
    "outDir": "out",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "src/**/*.ts"],
  "exclude": ["node_modules"]
}
